# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_WS_BASE_URL=ws://localhost:8000

# App Configuration
VITE_APP_TITLE=AI Workspace
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_SERVICE_WORKER=false

# Cache Configuration
VITE_CACHE_TTL=300
VITE_CACHE_MAX_SIZE=100

# API Timeouts (in milliseconds)
VITE_API_TIMEOUT=30000
VITE_WS_TIMEOUT=35000

# Security Settings
VITE_ENABLE_CSP=false
VITE_ENABLE_RATE_LIMITING=false
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=60000

# PWA Settings
VITE_PWA_NAME=AI Workspace
VITE_PWA_SHORT_NAME=AIW
VITE_PWA_THEME_COLOR=#1976D2
VITE_PWA_BACKGROUND_COLOR=#FFFFFF

# Other Settings
VITE_DEFAULT_LOCALE=en
VITE_SUPPORTED_LOCALES=["en","zh"]
VITE_DEFAULT_THEME=system

# API Configuration
VITE_API_BASE_URL=https://api.aiworkspace.app
VITE_WS_BASE_URL=wss://api.aiworkspace.app

# App Configuration
VITE_APP_TITLE=AI Workspace
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=production

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_SERVICE_WORKER=true

# Cache Configuration
VITE_CACHE_TTL=3600
VITE_CACHE_MAX_SIZE=1000

# API Timeouts (in milliseconds)
VITE_API_TIMEOUT=10000
VITE_WS_TIMEOUT=15000

# Security Settings
VITE_ENABLE_CSP=true
VITE_ENABLE_RATE_LIMITING=true
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=60000

# PWA Settings
VITE_PWA_NAME=AI Workspace
VITE_PWA_SHORT_NAME=AIW
VITE_PWA_THEME_COLOR=#1976D2
VITE_PWA_BACKGROUND_COLOR=#FFFFFF
